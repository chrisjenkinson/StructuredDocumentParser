{
  "name": "chrisjenkinson/structured-document-parser",
  "description": "A structured document parser.",
  "type": "library",
  "require": {
    "php": "^7.1",
    "ext-libxml": "*"
  },
  "require-dev": {
    "ext-mbstring": "*",
    "phpspec/phpspec": "*",
    "phpunit/phpunit": "*",
    "leanphp/phpspec-code-coverage": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Jenkinson",
      "email": "chris@chrisjenkinson.org"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "chrisjenkinson\\StructuredDocumentParser\\": "src"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "scripts": {
    "test": [
      "bin/phpspec run",
      "bin/phpunit tests/"
      ]
  }
}
